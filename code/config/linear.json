{
    "Experiment": {
        "version": 0.0,
        "description": "Linear Tasks experiment using MAML.",
        "path": "../results"
    },
    "Dataset - metatrain": {
        "task": "linear",
        "param_centers": [2.0, 3.0],
        "std_dev": 0.2,
        "num_tasks": 10000
    },
    "Dataset - metatest": {
        "task": "linear",
        "param_centers": [2.0, 3.0],
        "std_dev": 0.2,
        "num_tasks": 1000
    },
    "Model": {
        "type": "linear",
        "args": {
            "input_dim": 1,
            "output_dim": 1
        }
    },
    "MAML": {
        "alpha": 0.01,
        "beta": 0.01,
        "inner_steps": 5,
        "inner_criterion": "mse",
        "optimizer": "adam"
    },
    "training": {
        "epochs": 1
    }
}